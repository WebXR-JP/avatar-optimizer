{
  "name": "@xrift/avatar-optimizer",
  "version": "0.1.0",
  "description": "3D model optimization library for WebXR applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "xrift-optimize": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "format": "prettier --write .",
    "test": "vitest run",
    "manual:loaders-gl": "tsx __tests__/manual/loaders-gl.manual.ts"
  },
  "keywords": [
    "vrm",
    "webxr",
    "optimization",
    "gltf",
    "3d"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@gltf-transform/core": "^4.0.0",
    "@gltf-transform/extensions": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@gltf-transform/core": "^4.0.0",
    "@gltf-transform/extensions": "^4.0.0",
    "@types/node": "^24.10.0",
    "@types/three": "^0.181.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gltf-validator": "2.0.0-dev.3.10",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.4",
    "vrm-validator": "^2.0.0-dev.3.10"
  },
  "dependencies": {
    "@loaders.gl/core": "^4.3.4",
    "@loaders.gl/gltf": "^4.3.4",
    "@loaders.gl/images": "^4.3.4",
    "@loaders.gl/polyfills": "^4.3.4",
    "@pixiv/three-vrm": "^3.4.4",
    "@pixiv/three-vrm-materials-mtoon": "^3.4.4",
    "@xrift/mtoon-instancing": "workspace:*",
    "canvas": "^3.2.0",
    "commander": "^14.0.2",
    "jimp": "^1.6.0",
    "neverthrow": "^8.2.0",
    "rectpack-ts": "^0.0.2",
    "three": "^0.181.1"
  }
}
