{
  "name": "@xrift/avatar-optimizer",
  "version": "0.1.0",
  "description": "3D model optimization library for WebXR applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "xrift-optimize": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "format": "prettier --write .",
    "test": "jest"
  },
  "keywords": [
    "vrm",
    "webxr",
    "optimization",
    "gltf",
    "3d"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@gltf-transform/core": "^4.0.0",
    "@gltf-transform/extensions": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@gltf-transform/core": "^4.0.0",
    "@gltf-transform/extensions": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.38.0",
    "canvas": "^3.2.0",
    "vrm-validator": "^2.0.0-dev.3.10"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "neverthrow": "^8.2.0",
    "@xrift/avatar-optimizer-texture-atlas": "workspace:*"
  }
}
